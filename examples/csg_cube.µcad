use std::math::*;
use std::ops::*;
use std::geo3d::*;

part CsgCube(size: Length) {
    s = size / sqrt(2.1);
    body = Sphere(r = s) & Cube(size);
    holes = Cylinder(h = size, d = s).orient([X,Y,Z]);
    body - holes;
}

CsgCube(50mm);
